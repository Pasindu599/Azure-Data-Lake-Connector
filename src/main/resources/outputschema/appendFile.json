{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Append File",
    "description": "Append a file.",
    "properties": {
        "payload": {
            "type": "boolean",
            "description": "Whether to append the file or not."
        },
        "attributes": {
            "type": "object",
            "description": "The file size of the file to be appended.",
            "properties": {
                "appendSize": {
                    "type": "integer",
                    "description": "The file size of the file to be appended."
                }
            }
        }
    },
    "required": [
        "payload",
        "attributes"
    ],
    "additionalProperties": false
}